#include<stdio.h>
void Selection_Sort(int A[],int B[],int n){
for(int i=0;i<n;i++){
int min=A[0];
int k=0;
for(int j=0;j<n;j++){
if(A[j]<temp){
min=A[j];
k=j;
}
}
B[i]=min;
A[k]=1e9;
}
}
int main(){
int n;
scanf("%d",&n);
int A[n];
int B[n];
for(int i=0;i<n;i++){
scanf("%d",&A[i]);
}
Selection_Sort(A,B,n);
for(int i=0;i<n;i++){
printf("%d",B[i]);
}
}
